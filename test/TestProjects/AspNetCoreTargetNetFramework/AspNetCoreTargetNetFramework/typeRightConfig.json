{
  "$schema": "https://raw.githubusercontent.com/someguy20336/TypeRight/tool/v1.4.0/src/TypeRight.Core/Configuration/typeRightConfig-schema.json",
  "enabled": true,
  "serverObjectsResultFilepath": "./Scripts/ServerObjects.ts",

  "fetchConfig": {
    "filePath": "./Scripts/Communication.ts",
    "name": "fetchWrapper",
    "returnType": "Promise<$returnType$>",
    "parameters": [
      "requestMethod",
      "url",
      "body",
      {
    	"name": "abort",
    	"type": "AbortSignal",
    	"optional": true
      }
    ]
  }
}
{
	// Set this to false to disable automatic generation
	"Enabled": true,

	// This is the template you would like to use.  Choose from "module" or "namespace".  Module is the recommended approach,
	// But you can see more details on project page
	"TemplateType": "module",

	// This is the relative path (from the csproj root) to the output file for classes, interfaces, and enums
	"ServerObjectsResultFilepath": "./Scripts/ServerObjects.ts",

	// Customize the AJAX function used by controllers here
	"AjaxFunctionName": "",
	"AjaxFunctionModulePath": "",

	// Valid values here are singleParam and multiParam.  singleParam is recommended, at least for ASP.NET Core projects, but it might
	// depend on how you make the requests.  See the GitHub project for more details
	"modelBindingType": "singleParam"

	// These settings are only applicable for the "namespace" template type.  Uncomment this if you want to use that template
	//"ClassNamespace": "ServerClasses",
	//"EnumNamespace": "ServerEnums",
	//"WebMethodNamespace": "WebMethods",

	// It isn't recommended you change this setting, but if you wanted to have another attribute instead of ScriptAction
	// mark MVC actions, you can do that by giving the fully qualified type name of the attribute
	//"MvcActionAttributeName": "",

}